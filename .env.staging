# Staging Environment Configuration
# Set these as environment variables in your staging deployment

# Database - PostgreSQL connection string for staging
DATABASE_URL="postgresql://user:password@host:5432/mindmap_staging"

# NextAuth Configuration - MUST match your staging domain
NEXTAUTH_URL="https://staging.yourdomain.com"
NEXTAUTH_SECRET="REPLACE_WITH_SECURE_SECRET_MIN_32_CHARS"

# Encryption for LLM API keys
ENCRYPTION_KEY="REPLACE_WITH_SECURE_KEY_MIN_32_CHARS"

# Redis (optional for staging)
REDIS_URL="redis://user:password@host:6379"

# Web search providers (optional)
# TAVILY_API_KEY="your-tavily-api-key"
# SERPAPI_API_KEY="your-serpapi-api-key"
# BING_SEARCH_API_KEY="your-bing-api-key"

# App Configuration
NEXT_PUBLIC_APP_NAME="MindMap Staging"

# CORS Configuration (optional)
# ALLOWED_ORIGINS="https://staging.yourdomain.com"
